{
	"name": "configuration-service",
	"version": "0.0.1",
	"description": "A1 config service",
	"main": "index.js",
	"scripts": {
		"lint": "npx eslint --fix",
		"prod": "set NODE_ENV=production && node index.js",
		"build": "webpack --config webpack.config.js",
		"start": "set NODE_ENV=development && nodemon -L --polling-interval 5000 --ignore logs --ignore dev --verbose --inspect=0.0.0.0:9222 --env-file=.env.local index.js",
		"check-packages": "ncu",
		"upgrade-packages": "ncu -u",
		"check-dependencies": "npx depcheck",
		"pi-report": "rm -rf node_modules && npx cloc .",
		"reinstall": "rm -rf node_modules && yarn install",
		"prettify": "prettier --write \"*.{js,jsx,ts,tsx}\"",
		"gen:swagger": "node ./server/infrastructure/swagger.js"
	},
	"repository": "https://dev.azure.com/A1OSSBG/SmartTVApp/_git/ConfigurationService",
	"author": "A1BG",
	"license": "UNLICENSED",
	"private": true,
	"dependencies": {
